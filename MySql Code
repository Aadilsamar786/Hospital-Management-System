create table Department(
department varchar(100),
Phone_no varchar(20));


create table EMP_INFO(
Name varchar(20),
Age varchar(10),
Phone_number varchar(20),
salary varchar(20),
Gmail varchar(20),
Adhar_Number varchar(20));

create table Ambulance(
Name varchar(20),
Gender varchar(20),
car_Name varchar(20),
Available varchar(20),
Location varchar(20));


