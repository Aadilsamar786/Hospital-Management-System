create table login(
ID varchar(20),
Pw varchar(20));

create table patient_info(ID varchar(20),
number varchar(40),
Name varchar(20),
Gender varchar(20),
Patient_Disease varchar(20),
Room_Number varchar(20),
Time varchar(100),
Deposite varchar(20)
);

create table room(room_no varchar(20),
Availability varchar(20),
Price varchar(20),
Room_type varchar(100)
);

